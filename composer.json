{
  "name": "yexiaoxiong/modules",
  "description": "Laravel Module management",
  "keywords": [
    "modules",
    "laravel",
    "hxcart",
    "module",
    "rad"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com",
      "homepage": "https://nicolaswidart.com",
      "role": "Developer"
    }
  ],
  "support": {
	"issues": "https://github.com/laravelbook/ardent/issues"
  },
  "require": {
    "php": ">=5.5",
    "laravel/framework": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/validation": "~5.1",
    "illuminate/events": "~5.1",
    "illuminate/hashing": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "^3.1|^3.2|^3.3|^3.4",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Hxcart\\Modules\\": "src",
      "Ardent\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Hxcart\\Modules\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hxcart\\Modules\\ModulesServiceProvider"
      ],
      "aliases": {
        "Module": "Hxcart\\Modules\\Facades\\Module"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
